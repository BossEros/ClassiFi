{
  "name": "classifi-backend-ts",
  "version": "1.0.0",
  "description": "ClassiFi Backend API - TypeScript with Fastify",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && npx tsc-alias",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/multipart": "^9.4.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@supabase/supabase-js": "^2.81.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.36.4",
    "fastify": "^5.7.1",
    "fastify-plugin": "^5.1.0",
    "fastify-type-provider-zod": "^6.1.0",
    "postgres": "^3.4.5",
    "reflect-metadata": "^0.2.2",
    "tree-sitter": "^0.21.1",
    "tree-sitter-c": "^0.21.0",
    "tree-sitter-java": "^0.23.5",
    "tree-sitter-python": "^0.21.0",
    "tsyringe": "^4.10.0",
    "zod": "^4.3.5",
    "uuid": "^13.0.0",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^4.0.17",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.17.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
